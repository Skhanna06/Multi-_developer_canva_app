            <param style="plain" name="parameters" path="/parameters" type="service:ExecuteSqlStatement_param_body_def_parameters_def" />
          </representation>
        </request>
        <response>
          <representation mediaType="application/json" element="service:ExecuteSqlStatement_responses_200_def" />
        </response>
      </method>
    </resource>
