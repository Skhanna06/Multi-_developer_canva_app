          <representation mediaType="application/json">
            <param style="plain" name="statement" path="/statement" type="xs:string" />
            <param style="plain" name="timeout" path="/timeout" type="xs:long" default="60" />
            <param style="plain" name="database" path="/database" type="xs:string" />
            <param style="plain" name="schema" path="/schema" type="xs:string" />
